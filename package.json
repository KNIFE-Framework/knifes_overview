{
  "name": "knife_preview",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "dev": "docusaurus start",
    "knife:validate": "node scripts/validate_knifes_csv.mjs",
    "knife:validate:strict": "node scripts/validate_knifes_csv.mjs --strict",
    "knife:generate": "node scripts/knifes-generate.mjs",
    "knife:prepare": "npm run knife:validate && npm run knife:generate",
    "knife:prepare:strict": "npm run knife:validate:strict && npm run knife:generate",
    "knife:prepare:demo": "cross-env KNIFES_CSV=data/KNIFES-OVERVIEW-INPUTS.csv KNIFES_LOCALE=sk KNIFES_ROOT=. npm run knife:prepare",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus start",
    "start:sk": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus start --locale sk --no-open",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build --no-minify",
    "build:ci": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build --no-minify",
    "serve": "docusaurus serve",
    "prod": "npm run build && npm run serve",
    "lint:css": "stylelint \"src/**/*.css\" \"docs/**/*.css\"",
    "lint": "npm run lint:css",
    "knife:gen": "node scripts/knifes-generate.mjs",
    "knife:norm": "node scripts/knifes-normalize.mjs",
    "knife:update": "node scripts/knifes-update-by-guid.mjs --policy add-only"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/plugin-google-gtag": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/plugin-client-redirects": "^3.9.1",
    "@docusaurus/tsconfig": "^3.9.1",
    "@docusaurus/types": "^3.9.1",
    "cross-env": "^7.0.3",
    "cssnano": "6.0.1",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  },
  "overrides": {
    "cssnano": "6.0.1"
  }
}
