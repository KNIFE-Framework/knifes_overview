{
  "name": "knife_preview",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "dev": "docusaurus start",
    "knifes:gen": "node scripts/build_knifes.mjs --csv data/KNIFE-OVERVIEW-ONLY.csv --root .",
    "knifes:gen:dry": "node scripts/build_knifes.mjs --csv data/KNIFE-OVERVIEW-ONLY.csv --root . --dry-run",
    "knifes:verify": "node scripts/knife_verify.mjs --csv data/KNIFE-OVERVIEW-ONLY.csv --root .",
    "knifes:verify:dry": "node scripts/knife_verify.mjs --csv data/KNIFE-OVERVIEW-ONLY.csv --root . --dry-verify",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus start",
    "start:sk": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus start --locale sk --no-open",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build --no-minify",
    "build:ci": "cross-env NODE_OPTIONS=--max-old-space-size=16384 docusaurus build --no-minify",
    "serve": "docusaurus serve",
    "prod": "npm run build && npm run serve"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.0",
    "@docusaurus/plugin-google-gtag": "^3.9.0",
    "@docusaurus/preset-classic": "^3.9.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.0",
    "@docusaurus/plugin-client-redirects": "^3.9.0",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "cross-env": "^7.0.3",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
